{
    "name": "Mutual Friends Plugin",
    "version": "0.3",
    "manifest_version": 2,
    "description": "View the mutual friends for a specific player.",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "*://*.roblox.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["*://*.roblox.com/*"],
            "run_at": "document_end",
            "js": ["plugin.js"]
        }
    ]
}
